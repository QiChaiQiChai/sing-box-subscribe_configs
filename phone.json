      {
        "tag": "remote-dns",
        "address": "https://8.8.8.8/dns-query",
        "detour": "📦"
      },
      {
        "tag": "local-dns",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "domain": [
          "ghproxy.com",
          "cdn.jsdelivr.net",
          "testingcf.jsdelivr.net"
        ],
        "server": "local-dns"
      },
      {
        "outbound": "any",
        "disable_cache": true,
