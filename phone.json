        "protocol": "dns",
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "network": "udp",
        "port": 443,
        "outbound": "block"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "outbound": "block"
      },
      {
        "domain_suffix": [
          "appcenter.ms",
          "firebase.io",
          "crashlytics.com"
