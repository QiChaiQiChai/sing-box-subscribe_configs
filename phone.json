        "outbound": "📦"
      },
      {
        "rule_set": [
          "geoip-google"
        ],
        "outbound": "📦"
      },
      {
        "rule_set": [
          "geosite-openai"
        ],
        "outbound": "📦"
      },
      {
        "rule_set": [
